{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "create:db": {
      "cache": false
    },
    "start:db": {
      "cache": false,
      "dependsOn": ["create:db"]
    },
    "db:reset": {
      "cache": false,
      "dependsOn": ["start:db"]
    },
    "start:dev": {
      "dependsOn": ["db:reset"]
    },
    "build": {
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "type-check": {}
  }
}